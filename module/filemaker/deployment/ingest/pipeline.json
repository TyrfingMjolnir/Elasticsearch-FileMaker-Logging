{
	"description": "Pipeline for parsing filemaker deployment logs",
	"on_failure": [{
		"set": {
			"field": "error",
			"value": "{{ _ingest.on_failure_message }}"
		}
	}],
	"processors": [{
			"grok": {
				"field": "message",
				"ignore_missing": true,
				"pattern_definitions": {
					"GREEDYMULTILINE": "(.|\n)*",
					"TAB": "\t",
					"TOMCAT_DATESTAMP": "20%{YEAR}-%{MONTHNUM}-%{MONTHDAY} %{HOUR}:?%{MINUTE}(?::?%{SECOND}) %{ISO8601_TIMEZONE}"
				},
				"patterns": [
"%{TOMCAT_DATESTAMP:filemaker.deployment.logtime}%{TAB}%{WORD:filemaker.deployment.logtype}%{TAB}%{WORD:filemaker.deployment.logid}%{TAB}%{HOSTNAME:filemaker.deployment.hostname}%{TAB}%{HOSTNAME} \"%{DATA:filemaker.deployment.client}\" %{WORD:filemaker.deployment.type} %{DATA}\"%{DATA:filemaker.deployment.hostnamex} \\(%{DATA:filemaker.deployment.ip}\\)\" %{DATA} \"%{DATA:filemaker.deployment.app}",
					
"%{TOMCAT_DATESTAMP:filemaker.deployment.logtime}%{TAB}%{WORD:filemaker.deployment.logtype}%{TAB}%{WORD:filemaker.deployment.logid}%{TAB}%{HOSTNAME:filemaker.deployment.hostname}%{TAB}%{HOSTNAME} \"%{DATA:filemaker.deployment.client}\\(%{DATA:filemaker.deployment.hostnamex}\\) \\[%{DATA:filemaker.deployment.ip}\\]\" %{DATA} \"%{DATA:filemaker.deployment.database}\" %{DATA}\"%{DATA:filemaker.deployment.user}\"?",

"%{TOMCAT_DATESTAMP:filemaker.deployment.logtime}%{TAB}%{WORD:filemaker.deployment.logtype}%{TAB}%{WORD:filemaker.deployment.logid}%{TAB}%{HOSTNAME:filemaker.deployment.hostname}%{TAB}%{HOSTNAME} \"%{DATA:filemaker.deployment.client}\\(%{DATA:filemaker.deployment.hostnamex}\\) \\[%{DATA:filemaker.deployment.ip}\\]\" %{DATA}."


				]
}
		},
		{
			"remove": {
				"field": "messagez"
			}
		},
		{
  			  "geoip": {
 			     "field": "filemaker.deployment.ip",
   			   "target_field": "filemaker.deployment.geoip"
    }		},
		{
			"date": {
				"field": "filemaker.deployment.logtime",
				"formats": [
					"MMM  d HH:mm:ss",
					"MMM dd HH:mm:ss"
				],
				"ignore_failure": true,
				"target_field": "@timestamp"
			}
		}
	]
}
